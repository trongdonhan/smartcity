															<button type="button" class="btnadd btn btn-primary" data-toggle="modal"
															data-target="#ModalUpload<%=rows.idVi%>">Thêm hình</button>
													<%}%>
											</div>
										</div>
								</div>
							</td>
							<!-- Modal Upload -->
							<div class="modal fade" id="ModalUpload<%=rows.idVi%>" role="dialog">
									<div class="modal-dialog modal-sm modal-sm1">
										<div class="modal-content">
											<form role="form" action="uploadImageXP/<%=rows.idVi%>" method="POST" enctype="multipart/form-data">
												<div class="modal-header">
													<div class="lbl row">
														<div class="col-xs-12 col-sm-12 col-md-12">
															<h4><b>Hình ảnh hiện trạng công trình</b></h4>
														</div>
													</div>
												</div>
												<div class="modal-body">
													<div class="row">
														<div class="col-xs-12 col-sm-12 col-md-12 table-bordered">
															<div class="col-xs-push-4 col-sm-4 col-md-push-4 col-xs-8 col-sm-8 col-md-8">
																<input id="files<%=i-1%>" type="file" name="upload" /><br/>
																<label style="color: red"><i>**Chỉ chọn một hình</i></label>
															</div>
															<div class="col-xs-push-2 col-sm-2 col-md-push-2 col-xs-8 col-sm-8 col-md-8">
																<output id="result<%=i-1%>"></output>
															</div>
														</div>
													</div>
												</div>
												<div class="modal-footer">
													<button type="submit" class="btn1 btn btn-primary">Lưu</button>
												</div>
											</form>
										</div>
									</div>
								</div>
							<!-- Modal Edit -->
							<div class="modal fade" id="ModalEdit<%=rows.idVi%>" role="dialog">
								<div class="modal-dialog modal-sm">
									<div class="modal-content">
										<div class="modal-header">
											<div class="lbl row">
												<div class="col-xs-6 col-md-6">
													<h4><b>Thông tin chủ đầu tư</b></h4>
												</div>
												<div class="col-xs-6 col-md-6">
													<h4><b>Thông tin công trình</b></h4>
												</div>
											</div>
										</div>
										<div class="modal-body">
											<form role="form" action="suavipham" method="POST">
												<div class="row">
													<div class="group1 col-md-6">
														<div class="form-group">
															<label>Họ và tên</label>
															<input type="text" class="form-control"
																value="<%=rows.hoten%>" name="hoten">
														</div>
														<div class="form-group">
															<label>CMND/Thẻ căn cước</label>
															<input type="text" class="form-control"
																value="<%=rows.cmnd%>" name="cmnd" readonly>
														</div>
														<div class="form-group">
															<label>Ngày cấp</label>
															<!-- <h5 id="ngayCap<%=rows.idVi%>" class="form-control"></h5> -->
															<input type="date" class="form-control"
																id="ngaycap<%=rows.idVi%>" name="ngaycap">
														</div>
														<div class="form-group">
															<label>Nơi cấp</label>
															<input type="text" class="form-control"
																value="<%=rows.diachicap%>" name="diachicap">
														</div>
														<div class="form-group">
															<label>Số điện thoại:</label>
															<input type="text" class="form-control"
																value="<%=rows.sdt%>" name="sdt">
														</div>
														<div class="form-group">
															<label>Ngày sinh</label>
															<input type="date"
																class="form-control contentNS<%=rows.idVi%>"
																id="ngaysinh<%=rows.idVi%>" name="ngaysinh">
														</div>
														<div class="form-group">
															<label>Giới tính</label>
															<% if (rows.gioitinh == 1) {%>
															<input name="gioitinh" type="radio" value=1 checked />Nam
															<input name="gioitinh" type="radio" value=0 />Nữ
															<%} else {%>
															<input name="gioitinh" type="radio" value=1 />Nam
															<input name="gioitinh" type="radio" value=0 checked />Nữ
															<%}%>
														</div>

														<div class="form-group">
															<label>Địa chỉ thường trú</label>
															<input type="text" class="form-control"
																value="<%=rows.diachiTT%>" name="diachiTT">
														</div>
														<div class="form-group">
															<label>Nghề nghiệp:</label>
															<input type="text" class="form-control"
																value="<%=rows.nghenghiep%>" name="nghenghiep">
														</div>
														<div class="form-group">
															<label>Quốc tịch</label>
															<input type="text" class="form-control"
																value="<%=rows.quoctich%>" name="quoctich" readonly>
														</div>
													</div>
													<div class="group2 col-md-6">
														<div class="form-group">
															<label>Mã công trình</label>
															<!-- <h5>&nbsp;&nbsp;<%=rows.idVi%></h5> -->
															<input type="text" class="form-control" value="<%=rows.idVi%>"
																name="idVP" readonly>
															<label>Số biên bản</label>
															<input type="text" class="form-control"
																value="<%=rows.soBB%>" name="soBB">
															<label>Địa chỉ</label>
															<input type="text" class="form-control"
																value="<%=rows.diachiVP%>" name="diachiVP">
														</div>
														<div class="form-group">
															<label>Hình ảnh công trình</label></br>
															<% if (rows.url == null) {%>
															<label style="color: red">**Hiện chưa có hình ảnh</label>
															<%} else {%>
															<iframe src="<%=rows.url%>" width="250" height="150" aria-disabled="true"></iframe>
															<%}%>
														</div>
														<div class="form-group">
															<label>Thời điểm vi phạm</label>
															<input type="date"
																class="form-control contentVP<%=rows.idVi%>"
																id="ngayvp<%=rows.idVi%>" name="thoigianVP">
														</div>
														<div class="form-group">
															<label>Giờ lập biên bản</label>
															<input type="time"
																class="form-control"
																value="<%=rows.gioLBB%>" name="gioLBB">
														</div>
														<div class="form-group">
															<label>Ngày lập biên bản</label>
															<input type="date"
																class="form-control contentLBB<%=rows.idVi%>"
																id="ngaylbb<%=rows.idVi%>" name="thoigianLBB">
														</div>

														<div class="form-group">

															<label>Nội dung xử phạt</label>
															<%results_content.forEach(function(rows_content){
																	if (rows_content.id_violation === rows.idVi){ %>
															<input type="text" class="form-control"
																value="<%=rows_content.noidung%>" name="noidungVP"
																class="contentEdit">
															<input type="hidden" class="form-control"
																value="<%=rows_content.id%>" name="idcontent">

															<%}});%>
														</div>
														<div class="form-group">
																<label>Hiện trạng công trình</label>
																<input type="text"
																	class="form-control"
																	value="<%=rows.hientrangCT%>" name="hientrangCT">
															</div>
														<div class="form-group">
															<div class="row">
																<div class="col-xs-3 col-md-3">
																	<label>Số tiền phạt</label>
																</div>
																<div class="col-xs-7 col-md-7">
																	<% if (rows.tienphat == 0) {%>
																	<input name="tienphat" type="radio" value=0
																		checked /> 25.000.000 (Hai mươi lăm triệu
																	đồng)</br>
																	<input name="tienphat" type="radio" value=1 />
																	35.000.000 (Ba mươi lăm triệu đồng)</br>
																	<input name="tienphat" type="radio" value=2 />
																	75.000.000 (Bảy mười lăm triệu đồng)</br>
																	<%} else if(rows.tienphat == 1) {%>
																	<input name="tienphat" type="radio" value=0 />
																	25.000.000 (Hai mươi lăm triệu đồng)</br>
																	<input name="tienphat" type="radio" value=1
																		checked /> 35.000.000 (Ba mươi lăm triệu
																	đồng)</br>
																	<input name="tienphat" type="radio" value=2 />
																	75.000.000 (Bảy mười lăm triệu đồng)</br>
																	<%} else {%>
																	<input name="tienphat" type="radio" value=0 />
																	25.000.000 (Hai mươi lăm triệu đồng)</br>
																	<input name="tienphat" type="radio" value=1 />
																	35.000.000 (Ba mươi lăm triệu đồng)</br>
																	<input name="tienphat" type="radio" value=2
																		checked /> 75.000.000 (Bảy mười lăm triệu
																	đồng)</br>
																	<%}%>
																</div>
															</div>
														</div>

														<script>
															function convert(str) {
																var date = new Date(str),
																	mnth = ("0" + (date.getMonth() + 1)).slice(-2),
																	day = ("0" + date.getDate()).slice(-2);
																return [date.getFullYear(), mnth, day].join("-");
															}
															function convertDayYear(str) {
																var date = new Date(str),
																	mnth = ("0" + (date.getMonth() + 1)).slice(-2),
																	day = ("0" + date.getDate()).slice(-2);
																return [date.getFullYear(), mnth, day].join("-");
															}
															var ngayCap = convert('<%=rows.ngaycap%>');
															var ngaySinh = convert('<%=rows.ngaysinh%>');
															var ngayVP = convert('<%=rows.thoigianVP%>');
															var ngayLBB = convert('<%=rows.thoigianLBB%>');

															//document.getElementById("ngayCap<%=rows.idVi%>").innerHTML

															document.getElementById("ngaycap<%=rows.idVi%>").value
																= ngayCap;
															//document.getElementById("ngaySinh<%=rows.idVi%>").innerHTML=
															ngaySinh;
															document.getElementById("ngaysinh<%=rows.idVi%>").value
																=
																ngaySinh;
															//document.getElementById("ngayVP<%=rows.idVi%>").innerHTML

															document.getElementById("ngayvp<%=rows.idVi%>").value
																=
																ngayVP;
															//document.getElementById("ngayLBB<%=rows.idVi%>").innerHTML

															document.getElementById("ngaylbb<%=rows.idVi%>").value
																=
																ngayLBB;
														</script>
																</div>
															</div>
														</div>
														<div class="modal-footer">
															<button type="submit"
																class="btn1 btn btn-primary">Lưu</button>
														</div>
											</form>
										</div>
									</div>
								</div>
						</tr>
						<%});%>
					</table>
				</div>
		</section>
	</div>
</body>

</html>